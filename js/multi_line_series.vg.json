{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 900,
    "height": 600,
    "padding": {"left": 10},
    "data": {"url": "https://raw.githubusercontent.com/qwan0066/fit3179-assignment2/main/data/day_wise.csv"},
    "transform": [
        {"fold": ["Deaths / 100 Cases", "Recovered / 100 Cases", "Deaths / 100 Recovered"]}
      ],
    "mark": {
      "type": "line"
    },
    "encoding": {
      "x": {"timeUnit": "monthdate","field": "Date", "type": "temporal"},
      "y": {"field": "value", "type": "quantitative"},
      "color": {"field": "key", "type": "nominal"}
    }
    
  }
  